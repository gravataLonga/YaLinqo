{
    "name": "gravatalonga/yalinqo",
    "description": "YaLinqo, a LINQ-to-objects library for PHP. Forked for purpose of compatibility with PHP 8.3, if you want this merge to YoLinqo just send me a email. I'm was in hurry, sorry. :(",
    "license": "BSD-2-Clause",
    "homepage": "http://athari.github.io/YaLinqo",
    "keywords": ["linq", "linqo", "query", "statistic"],
    "authors": [
        {
            "name": "Alexander Prokhorov",
            "homepage": "https://github.com/Athari/"
        },
        {
            "name": "Jonathan Fontes",
            "homepage": "https://github.com/gravataLonga/"
        }
    ],
    "support": {
        "issues": "https://github.com/Athari/YaLinqo/issues",
        "source": "https://github.com/Athari/YaLinqo",
        "docs": "http://athari.github.io/YaLinqo"
    },
    "require": {
        "php": ">=8.3",
        "php5friends/polyfill-create_function": "^0.0.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^12.1",
        "rector/rector": "*"
    },
    "autoload": {
        "psr-4": {
            "Gravatalonga\\YaLinqo\\": "src/"
        },
        "files": [
            "src/Linq.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "YaLinqo\\Tests\\": "Tests/"
        }
    },
    "scripts": {
        "rector": "/vendor/bin/rector"
    }
}
